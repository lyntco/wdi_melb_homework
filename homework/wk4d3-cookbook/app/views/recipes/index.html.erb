<% end %>

<div class="row">
  <div class="col-md-6"><%= link_to 'new book', new_book_path, class: 'btn btn-primary' %></div>
  <div class="col-md-6"><%= link_to 'new recipe', new_recipe_path, class: 'btn btn-primary' %></div>
</div>