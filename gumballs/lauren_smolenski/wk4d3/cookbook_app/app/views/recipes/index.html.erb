<h1>All Recipes</h1>
<% @recipes.each do |recipe| %>
<p><%= recipe.name %></p>
<img src="<%= recipe.image_url %>" alt="">
<p><%= link_to 'show', recipe %></p>
<p><%= link_to 'edit', edit_recipe_path(recipe) %></p>
<p><%= link_to 'delete', recipe, :method => :delete, :data => { :confirm => 'are you sure you want to delete this recipe?' } %></p>
<% end %>